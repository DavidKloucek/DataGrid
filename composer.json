{
	"name": "davidkloucek/grid-project",
	"keywords": ["nette", "grid", "datagrid", "react"],
	"type": "project",
	"license": ["MIT"],
	"require": {
		"php": "8.1.4",
		"nette/nette": "^3.1",
		"tracy/tracy": "^2.9",
		"symfony/validator": "^5.4",
		"symfony/property-access": "^5.4",
		"beberlei/doctrineextensions": "^1.3",
		"contributte/console": "^0.9.1",
		"contributte/event-dispatcher": "^0.8.1",
		"nettrine/orm": "^0.8.3"
    },
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"platform": {
			"php": "8.1.4"
		},
		"optimize-autoloader": true
	},
	"require-dev": {
		"rector/rector": "^0.12.19",
		"nette/tester": "^2.4",
		"roave/security-advisories": "dev-latest"
	},
	"autoload": {
		"psr-4": {
			"DemoApp\\": "app/",
			"Tests\\": "tests/"
		}
	}
}
